
public class InvCount {

	public static void main(String[] args) {
		

	}

	public static int countInversions(int[] C, int D[])
	{
		int i = 1;
		int j = 1;
		int splitInv = 0;
		int[] B = new int[C.length + D.length];
		for(int k = 1; k <= C.length + D.length; k++)
		{
			if(i < C.length && C[i] < D[j])
			{
				B[k] = C[i];
				i++;
			}
			else
			{
				B[k] = D[j];
				j++;
				splitInv++;
			}
		}
	}
}
